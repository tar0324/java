	<update id="updateBalance1">
		<![CDATA[
			update cust_account
			set balance = balance - 5000000
			where
			 accountNo = '70-490-930'
		]]>
	</update>
	
		<update id="updateBalance2">
		<![CDATA[
			update cust_account
			set balance = balance + 5000000
			where
			 accountNo = '70-490-911'
		]]>
	</update>
